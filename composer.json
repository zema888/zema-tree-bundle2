{
    "name": "zema888/zema-tree-bundle2",
    "type": "symfony-bundle",
    "description": "This Symfony bundle integrates angular matrial tree and Gedmo Nested Set directly to Sonata Admin",
    "keywords": ["ajax", "asynchronous", "async", "tree", "nested", "set", "bundle", "symfony", "bundle", "doctrine", "sonataproject"],
    "homepage": "https://github.com/zema888/zema-tree-bundle2",
    "license": "MIT",
    "authors": [
        {
            "name": "Vitaly Zemtsov",
            "homepage": "https://github.com/zema888"
        }
    ],
    "autoload": {
        "psr-4": {
            "zema888\\ZemaTreeBundle\\": "src"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.6.0",
        "stof/doctrine-extensions-bundle": "^1.2",
        "sonata-project/admin-bundle": "^3.1"
    },
    "target-dir": "zema888/ZemaTreeBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "0.1"
        }
    }
}
